				},
				{
					"name": "variable.parameter.langums.unitproperty",
					"match": "\\b(HitPoints|Health|ShieldPoints|Shields|Energy|ResourceAmount|HangarCount|Cloaked|Burrowed|InTransit|Hallucinated|Invincible)\\b"
				},
